{
  "dockerImageBuilder": "ritclizup/rit-python3-builder",
  "inputs": [
    {
      "label": "Type your name: ",
      "name": "rit_input_text",
      "required": true,
      "tutorial": "Your prefer name",
      "type": "text"
    },
    {
      "default": "false",
      "items": [
        "false",
        "true"
      ],
