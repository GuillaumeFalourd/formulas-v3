      "type": "bool"
    },
    {
      "default": "everything",
      "items": [
        "daily tasks",
        "workflows",
        "toils",
        "everything"
      ],
      "label": "What do you want to automate? ",
      "name": "rit_input_list",
      "required": false,
      "tutorial": "Select what you want to automate with Ritchie",
      "type": "text"
    }
  ],
  "template": "python3",
  "templateRelease": "2.16.2"
}
